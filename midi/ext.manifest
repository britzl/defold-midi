name: "DefoldMIDI"
platforms:

  x86_64-linux:
    context:
      defines: ["__LINUX_ALSA__"]
      libs: ["asound" , "pthread"]

  js-web:
    context:
      defines: ["__WEB_MIDI_API__"]

  wasm-web:
    context:
      defines: ["__WEB_MIDI_API__"]

  x86_64-win32:
    context:
      defines: ["__WINDOWS_MM__"]
      libs: ["winmm.lib"]

  x86-win32:
    context:
      defines: ["__WINDOWS_MM__"]
      libs: ["winmm.lib"]